*
	margin  0 
	padding 0
	-webkit-box-sizing: border-box;
	   -moz-box-sizing: border-box;
	        box-sizing: border-box;

.container-l
	width 	100%
	height 	100%

	&:first-of-type
		background-color white

	&:nth-of-type(2)
		background-color #b2d45b

	&:nth-of-type(3)
		background-color white

	&:nth-of-type(4)
		background-color #7dbeba

	&:nth-of-type(5)
		background-color #272526